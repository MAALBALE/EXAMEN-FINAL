    private void BtnGuardarActionPerformed(java.awt.event.ActionEvent evt) {                                           
       
        String a=txtNombre.getText();
        String c=txtCurso.getText();
        String b=txtId.getText();
        String d=txtNota.getText();
        
        int e=Integer.parseInt(b);
        int f=Integer.parseInt(d);
        
        Pricipal.guardar(a, c, e,f);
        
        txtNombre.setText("");
        txtId.setText("");
        txtCurso.setText("");
        txtNota.setText("");
        
        
    }                                          

    private void BtnRegresarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        
        Panel OpR = new Panel ();
        OpR.setVisible(true);
        this.dispose();
        
        // TODO add your handling code here:
    }                                           

